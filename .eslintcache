            setUrl(data.url);\n        })\n        .catch(err=>{\n            console.log(err);\n        })\n\n        \n    }\n\n    return (\n        <div className=\"card input-field\"\n                style={{\n                    margin:\"30px auto\",\n                    maxWidth :\"500px\",\n                    padding :\"20px\",\n                    textAlign: \"center\"\n                }}>\n            <input type=\"text\" placeholder=\"title\" value={title} onChange={(e)=>setTitle(e.target.value)}/>\n            <input type=\"text\" placeholder=\"body\" value={body} onChange={(e)=>setBody(e.target.value)}/>\n            <div className=\"file-field input-field\">\n                <div className=\"btn #64b5f6 blue darken-1\">\n                    <span>image</span>\n                    <input type=\"file\"  onChange={(e)=>setImage(e.target.files[0])}/>\n                </div>\n                <div className=\"file-path-wrapper\">\n                    <input className=\"file-path validate\" type=\"text\"/>\n                </div>\n            </div>\n            <button className=\"btn waves-effect waves-light #64b5f6 blue darken-1\" onClick={()=>postDetails()}>Post</button>\n\n        </div>\n    )\n}\n\nexport default CreatePost\n","/home/ved/nodeJS/Insta/instagram/src/reducers/userReducer.js",["62"],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":19,"column":12,"nodeType":"69","messageId":"70","endLine":19,"endColumn":17},{"ruleId":"71","severity":1,"message":"72","line":30,"column":7,"nodeType":"73","endLine":30,"endColumn":9,"suggestions":"74"},{"ruleId":"67","severity":1,"message":"75","line":6,"column":20,"nodeType":"69","messageId":"70","endLine":6,"endColumn":28},{"ruleId":"76","severity":1,"message":"77","line":11,"column":21,"nodeType":"78","endLine":12,"endColumn":196},{"ruleId":"76","severity":1,"message":"77","line":24,"column":17,"nodeType":"78","endLine":24,"endColumn":214}